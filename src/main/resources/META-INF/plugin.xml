<idea-plugin>
    <id>com.nisecoder.intellij.nuxtjs-intellij-plugin</id>
    <name>NuxtJS</name>
    <vendor email="nise.nabe@gmail.com" url="https://www.nisecoder.com">nise_nabe</vendor>

    <dependencies>
        <plugin id="JavaScript" />
    </dependencies>

    <extensions defaultExtensionNs="com.intellij">
        <createDirectoryCompletionContributor implementation="com.nisecoder.intellij.plugins.nuxtjs.NuxtJSDirectoryCompletionContributor"/>
        <directoryProjectGenerator implementation="com.nisecoder.intellij.plugins.nuxtjs.cli.NuxtJSCliProjectGenerator" />
        <projectTemplatesFactory implementation="com.nisecoder.intellij.plugins.nuxtjs.cli.NuxtJSProjectTemplateFactory" />
    </extensions>

    <actions>
        <!-- Add your actions here -->
    </actions>
</idea-plugin>
